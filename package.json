{
  "name": "etoken-lib",
  "version": "0.0.8",
  "description": "Ambisafe Etoken Lib",
  "main": "dist/index.js",
  "repository": "https://bitbucket.org/ambisafe/etoken-lib",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src --out-dir dist && webpack --config webpack.config.js",
    "build-min": "babel src --out-dir dist && webpack --config webpack-min.config.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "contract-container-storage-js": "0.0.4",
    "ambisafe-client-javascript": "0.11.4",
    "crypto": "0.0.3",
    "ethereumjs-tx": "^1.1.2",
    "ethereumjs-util": "4.3.0",
    "web3": "0.15.3",
    "web3-provider-engine": "Ambisafe/provider-engine.git#forgive-undef-strings"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.9.0",
    "babelify": "7.3.0",
    "install": "0.6.1",
    "json-loader": "0.5.4",
    "uglifyify": "3.0.2",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "browserify": {
    "extension": [
      ".es6"
    ],
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "add-module-exports"
          ]
        }
      ]
    ]
  }
}
