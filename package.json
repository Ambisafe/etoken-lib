{
  "name": "etoken-lib",
  "version": "0.17.2",
  "description": "Ambisafe Etoken Lib",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Ambisafe/etoken-lib.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src --out-dir dist && webpack --config webpack.config.js",
    "build-min": "babel src --out-dir dist && webpack --config webpack-min.config.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ambisafe-client-javascript": "0.11.4",
    "crypto": "0.0.3",
    "ethereumjs-tx": "1.3.3",
    "ethereumjs-util": "4.3.0",
    "web3": "0.20.7",
    "web3-provider-engine": "15.0.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "babel-loader": "^8.1.0",
    "babelify": "7.3.0",
    "install": "0.6.1",
    "json-loader": "0.5.4",
    "uglifyify": "^5.0.2",
    "webpack": "5.75.0",
    "webpack-dev-server": "4.11.1"
  },
  "browserify": {
    "extension": [
      ".es6"
    ],
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ],
          "plugins": [
            "add-module-exports"
          ]
        }
      ]
    ]
  }
