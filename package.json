{
  "name": "etoken-lib",
  "version": "0.0.3",
  "description": "Ambisafe Etoken Lib",
  "main": "dist/index.js",
  "repository": "https://bitbucket.org/ambisafe/etoken-lib",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src --out-dir dist && browserify --s EToken -t babelify -d --extension=.es6 src/index.es6 > build/bundle.js",
    "build-min": "browserify --s EToken -t babelify --extension=.es6 src/index.es6 | uglifyjs > build/bundle.min.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "contract-container-storage-js": "0.0.3",
    "ambisafe-client-javascript": "git://github.com/Ambisafe/client-javascript.git#d57d1754a11537073fdc715e0b6b386d23a9fc57",
    "crypto": "0.0.3",
    "ethereumjs-util": "4.3.0",
    "web3": "0.15.3",
    "web3-provider-engine": "7.1.1"
  },
  "devDependencies": {
    "babel-core": "6.7.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babelify": "7.2.0",
    "install": "0.6.1",
    "json-loader": "0.5.4",
    "uglifyify": "3.0.1",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "browserify": {
    "extension": [
      ".es6"
    ],
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "add-module-exports"
          ]
        }
      ]
    ]
  }
}
